        android:supportsRtl="true"
        android:theme="@style/Theme.Recycler19">

        <meta-data android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAr3e09QBzy4w9oYcjd58Fv6LJncO0jYnE"/>

        <activity android:name=".orgHomepage"></activity>
        <activity android:name=".userHomepage" />
        <activity android:name=".orgSignup" />
        <activity android:name=".orgLogin" />
        <activity android:name=".userSignup" />
        <activity android:name=".userLogin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>